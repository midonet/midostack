[project]
	description = OpenStack + MidoNet development environment
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group midonet-integration
[access "refs/heads/*"]
	read = group Anonymous Users
	forgeAuthor = group midonet-integration
	label-Code-Review = -2..+2 group midonet-integration
	label-Code-Review = -1..+1 group Anonymous Users
	forgeCommitter = group midonet-integration
[access "refs/tags/*"]
	push = group midonet-integration
	pushTag = group midonet-integration
	pushSignedTag = group midonet-integration
	read = group Anonymous Users
[receive]
	requireSignedOffBy = true
	requireChangeId = true
	createNewChangeForAllNotInTarget = true
[access "refs/for/refs/*"]
	read = group Anonymous Users
	push = group Anonymous Users
	pushMerge = group midonet-integration
	label-Code-Review = -2..+2 group midonet-integration
	label-Code-Review = -1..+1 group Anonymous Users
	rebase = group midonet-integration
	removeReviewer = group midonet-integration
	submit = group midonet-integration
