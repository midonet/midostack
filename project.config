[project]
	description = OpenStack + MidoNet development environment
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group midonet-integration
[access "refs/heads/*"]
	read = group Anonymous Users
	push = group midonet-integration
	pushMerge = group midonet-integration
	forgeAuthor = group midonet-integration
	label-Code-Review = -2..+2 group midonet-integration
	label-Code-Review = -1..+1 group Anonymous Users
[access "refs/tags/*"]
	push = +force group midonet
	pushTag = group midonet
	pushSignedTag = group midonet
[receive]
	requireSignedOffBy = true
	requireChangeId = true
	createNewChangeForAllNotInTarget = true
